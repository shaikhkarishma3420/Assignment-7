package PatternHomework;

public class pattern3 {
	public static void main(String[] args) {
		int n=5;
		for(int r=1;r<=5;r++) {
			for(int c=1;c<=5;c++) {
				if(r==c||r+c==n+1) {
					System.out.print("$ ");
				}
				else {
				System.out.print("* ");
			}
			}
			
			System.out.println();
			
		}
		
	}

}
