package PatternHomework;

public class printabcd {
	public static void main(String[] args) {
		char ch='A';
		for(char r=1;r<=5;r++) {
			ch='A';
			int sp=5;
			for(char k=1;k<=(sp-r);k++) {
				System.out.print(" ");
			}
			for(char c=1;c<=r;c++) {
				System.out.print(ch+" ");
				ch++;
			}
			System.out.println();
		}
	}

}
